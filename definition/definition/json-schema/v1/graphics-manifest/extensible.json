{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "http://127.0.0.1:8084/json-schema/v1/graphics-manifest/extensible.json",
    "allOf": [
        {
            "$ref": "http://127.0.0.1:8084/json-schema/v1/lib/common-graphics-schema.json"
        },
        {
            "type": "object",
            "properties": {
                "actions": {
                    "patternProperties": {
                        ".": {
                            "type": "object",
                            "$ref": "http://127.0.0.1:8084/json-schema/v1/lib/action.json"
                        }
                    }
                },
                "rendering": {
                    "type": "object",
                    "properties": {
                        "isExtensibleGraphic": {
                            "type": "boolean",
                            "const": true
                        }
                    },
                    "additionalProperties": true,
                    "required": [
                        "isExtensibleGraphic"
                    ]
                }
            },
            "additionalProperties": true
        }
    ]
}
